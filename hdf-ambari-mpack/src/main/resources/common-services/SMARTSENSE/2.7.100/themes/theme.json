{
   "name": "default",
   "description": "Default theme for SmartSense",
   "configuration": {
      "layouts": [
         {
            "name": "default",
            "tabs": [
               {
                  "name": "basic",
                  "display-name": "Basic",
                  "layout": {
                     "tab-columns": "2",
                     "tab-rows": "1",
                     "sections": [
                        {
                           "name": "section-account-info",
                           "display-name": "SmartSense Account",
                           "column-index": "0",
                           "column-span": "1",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "1",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-account-info-col1-row1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-account-info-col1-row2",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "1",
                                 "row-span": "1",
                                 "depends-on": [
                                    {
                                       "configs": [
                                          "hst-server-conf/customer.enable.flex.subscription"
                                       ],
                                       "if": "${hst-server-conf/customer.enable.flex.subscription}",
                                       "then": {
                                          "property_value_attributes": {
                                             "visible": true
                                          }
                                       },
                                       "else": {
                                          "property_value_attributes": {
                                             "visible": false
                                          }
                                       }
                                    }
                                 ]
                              }
                           ]
                        },
                        {
                           "name": "section-local-storage-info",
                           "display-name": "Local Storage",
                           "column-index": "1",
                           "column-span": "1",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "1",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-local-storage-info-col1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        }
                     ]
                  }
               },
               {
                  "name": "capture-controls",
                  "display-name": "Data Capture",
                  "layout": {
                     "tab-columns": "3",
                     "tab-rows": "1",
                     "sections": [
                        {
                           "name": "section-capture-controls",
                           "display-name": "Capture Controls",
                           "column-index": "0",
                           "column-span": "3",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "3",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-capture-controls-col1",
                                 "column-index": "0",
                                 "column-span": "3",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        }
                     ]
                  }
               },
               {
                  "name": "operations",
                  "display-name": "Operations",
                  "layout": {
                     "tab-columns": "3",
                     "tab-rows": "2",
                     "sections": [
                        {
                           "name": "section-server-operations",
                           "display-name": "Server Operations",
                           "column-index": "0",
                           "column-span": "3",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "2",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-server-operations-col1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-server-operations-col2",
                                 "column-index": "1",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        },
                        {
                           "name": "section-agent-operations",
                           "display-name": "Agent Operations",
                           "column-index": "0",
                           "column-span": "3",
                           "row-index": "1",
                           "row-span": "1",
                           "section-columns": "2",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-agent-operations-col1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-agent-operations-col2",
                                 "column-index": "1",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        }
                     ]
                  }
               },
               {
                  "name": "gateway",
                  "display-name": "Gateway",
                  "layout": {
                     "tab-columns": "1",
                     "tab-rows": "1",
                     "sections": [
                        {
                           "name": "section-gateway-info",
                           "display-name": "Gateway Configuration",
                           "column-index": "0",
                           "column-span": "1",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "3",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-gateway-info-col1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-gateway-info-col2",
                                 "column-index": "1",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-gateway-info-col3",
                                 "column-index": "2",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        }
                     ]
                  }
               },
               {
                  "name": "activity",
                  "display-name": "Activity Analysis",
                  "layout": {
                     "tab-columns": "2",
                     "tab-rows": "4",
                     "sections": [
                        {
                           "name": "section-activity-explorer",
                           "display-name": "Activity Explorer",
                           "column-index": "0",
                           "column-span": "2",
                           "row-index": "0",
                           "row-span": "1",
                           "section-columns": "3",
                           "section-rows": "1",
                           "subsections": [
                              {
                                 "name": "section-activity-explorer-col1",
                                 "column-index": "0",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-activity-explorer-col2",
                                 "column-index": "1",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              },
                              {
                                 "name": "section-activity-explorer-col3",
                                 "column-index": "2",
                                 "column-span": "1",
                                 "row-index": "0",
                                 "row-span": "1"
                              }
                           ]
                        }
                     ]
                  }
               }
            ]
         }
      ],
      "placement": {
         "configuration-layout": "default",
         "configs": [
            {
               "config": "hst-server-conf/customer.account.name",
               "subsection-name": "section-account-info-col1-row1"
            },
            {
               "config": "hst-server-conf/customer.smartsense.id",
               "subsection-name": "section-account-info-col1-row1"
            },
            {
               "config": "hst-server-conf/customer.notification.email",
               "subsection-name": "section-account-info-col1-row1"
            },
            {
               "config": "hst-server-conf/customer.enable.flex.subscription",
               "subsection-name": "section-account-info-col1-row1"
            },
            {
               "config": "hst-server-conf/customer.flex.subscription.id",
               "subsection-name": "section-account-info-col1-row2"
            },
            {
               "config": "hst-server-conf/server.storage.dir",
               "subsection-name": "section-local-storage-info-col1"
            },
            {
               "config": "hst-server-conf/server.tmp.dir",
               "subsection-name": "section-local-storage-info-col1"
            },
            {
               "config": "hst-agent-conf/agent.tmp.dir",
               "subsection-name": "section-local-storage-info-col1"
            },
            {
               "config": "hst-server-conf/server.port",
               "subsection-name": "section-server-operations-col1"
            },
            {
               "config": "hst-server-conf/agent.request.syncup.interval",
               "subsection-name": "section-server-operations-col2"
            },
            {
               "config": "hst-server-conf/agent.request.processing.timeout",
               "subsection-name": "section-server-operations-col2"
            },
            {
               "config": "hst-agent-conf/bundle.logs.to.capture",
               "subsection-name": "section-capture-controls-col1"
            },
            {
               "config": "hst-server-conf/client.threadpool.size.max",
               "subsection-name": "section-server-operations-col1"
            },
            {
               "config": "hst-agent-conf/server.connection.retry.count",
               "subsection-name": "section-agent-operations-col1"
            },
            {
               "config": "hst-agent-conf/server.connection.retry.interval",
               "subsection-name": "section-agent-operations-col1"
            },
            {
               "config": "hst-agent-conf/upload.retry.count",
               "subsection-name": "section-agent-operations-col2"
            },
            {
               "config": "hst-agent-conf/upload.retry.interval",
               "subsection-name": "section-agent-operations-col2"
            },
            {
               "config": "anonymization-rules/anonymization.rules.content",
               "subsection-name": "section-capture-controls-col1"
            },
            {
               "config": "hst-server-conf/gateway.host",
               "subsection-name": "section-gateway-info-col1"
            },
            {
               "config": "hst-server-conf/gateway.port",
               "subsection-name": "section-gateway-info-col2"
            },
            {
               "config": "hst-server-conf/gateway.registration.port",
               "subsection-name": "section-gateway-info-col3"
            },
            {
               "config": "activity-zeppelin-site/zeppelin.server.port",
               "subsection-name": "section-activity-explorer-col1"
            },
            {
               "config": "activity-zeppelin-site/zeppelin.anonymous.allowed",
               "subsection-name": "section-activity-explorer-col2"
            },
            {
               "config": "activity-zeppelin-shiro/users.admin",
               "subsection-name": "section-activity-explorer-col3"
            }
         ]
      },
      "widgets": [
         {
            "config": "hst-server-conf/customer.account.name",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/customer.smartsense.id",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/customer.notification.email",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/customer.enable.flex.subscription",
            "widget": {
               "type": "toggle"
            }
         },
         {
            "config": "hst-server-conf/customer.flex.subscription.id",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/server.storage.dir",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/server.tmp.dir",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-agent-conf/agent.tmp.dir",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/server.port",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/agent.request.syncup.interval",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Sec"
                  }
               ]
            }
         },
         {
            "config": "hst-server-conf/agent.request.processing.timeout",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Sec"
                  }
               ]
            }
         },
         {
            "config": "hst-agent-conf/bundle.logs.to.capture",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/client.threadpool.size.max",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Threads"
                  }
               ]
            }
         },
         {
            "config": "hst-agent-conf/upload.retry.count",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Attempts"
                  }
               ]
            }
         },
         {
            "config": "hst-agent-conf/upload.retry.interval",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Sec"
                  }
               ]
            }
         },
         {
            "config": "hst-agent-conf/server.connection.retry.count",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Attempts"
                  }
               ]
            }
         },
         {
            "config": "hst-agent-conf/server.connection.retry.interval",
            "widget": {
               "type": "slider",
               "units": [
                  {
                     "unit-name": "Sec"
                  }
               ]
            }
         },
         {
            "config": "anonymization-rules/anonymization.rules.content",
            "widget": {
               "type": "text-area"
            }
         },
         {
            "config": "hst-server-conf/gateway.host",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/gateway.port",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "hst-server-conf/gateway.registration.port",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "activity-zeppelin-site/zeppelin.server.port",
            "widget": {
               "type": "text-field"
            }
         },
         {
            "config": "activity-zeppelin-site/zeppelin.anonymous.allowed",
            "widget": {
               "type": "toggle"
            }
         },
         {
            "config": "activity-zeppelin-shiro/users.admin",
            "widget": {
               "type": "password"
            }
         }
      ]
   }
}