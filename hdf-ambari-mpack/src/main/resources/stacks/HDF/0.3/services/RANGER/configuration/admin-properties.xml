    <deleted>true</deleted>
  </property>

  <property>
    <name>xa_ldap_groupSearchFilter</name>
    <deleted>true</deleted>
  </property>

  <property>
    <name>xa_ldap_groupRoleAttribute</name>
    <deleted>true</deleted>
  </property>

  <property>
    <name>xa_ldap_ad_domain</name>
    <deleted>true</deleted>
  </property>

  <property>
    <name>xa_ldap_ad_url</name>
    <deleted>true</deleted>
  </property>

  <property>
    <name>SQL_COMMAND_INVOKER</name>
    <deleted>true</deleted>
